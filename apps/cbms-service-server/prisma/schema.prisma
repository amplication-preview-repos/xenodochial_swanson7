datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model WasteManagement {
  compostingPractices        Boolean?
  createdAt                  DateTime                                       @default(now())
  frequencyOfWasteCollection EnumWasteManagementFrequencyOfWasteCollection?
  hazardousWasteDisposal     EnumWasteManagementHazardousWasteDisposal?
  id                         String                                         @id @default(cuid())
  recyclingPractices         Boolean?
  updatedAt                  DateTime                                       @updatedAt
  wasteDisposalMethod        EnumWasteManagementWasteDisposalMethod?
  wasteSegregationPractices  Boolean?
}

model HouseholdsCharacteristics {
  createdAt               DateTime                                         @default(now())
  headOfHousehold         String?
  householdIncome         Float?
  id                      String                                           @id @default(cuid())
  mainSourceOfIncome      EnumHouseholdsCharacteristicsMainSourceOfIncome?
  monthlyExpenses         Float?
  numberOfDependents      Int?
  numberOfEmployedMembers Int?
  numberOfMembers         Int?
  primaryLanguageSpoken   String?
  updatedAt               DateTime                                         @updatedAt
}

model SalariesAndWages {
  createdAt          DateTime                                @default(now())
  employerName       String?
  frequencyOfPayment EnumSalariesAndWagesFrequencyOfPayment?
  id                 String                                  @id @default(cuid())
  jobTitle           String?
  monthlySalary      Float?
  otherBenefits      String?
  typeOfEmployment   EnumSalariesAndWagesTypeOfEmployment?
  updatedAt          DateTime                                @updatedAt
}

model HouseCharacteristics {
  constructionMaterial EnumHouseCharacteristicsConstructionMaterial?
  createdAt            DateTime                                      @default(now())
  electricityAccess    Boolean?
  houseType            EnumHouseCharacteristicsHouseType?
  id                   String                                        @id @default(cuid())
  numberOfRooms        Int?
  ownershipStatus      EnumHouseCharacteristicsOwnershipStatus?
  sanitationFacility   EnumHouseCharacteristicsSanitationFacility?
  updatedAt            DateTime                                      @updatedAt
  waterSupply          EnumHouseCharacteristicsWaterSupply?
  yearBuilt            Int?
}

model Demography {
  age                   Int?
  citizenship           String?
  createdAt             DateTime                             @default(now())
  disabilityStatus      Boolean?
  educationalAttainment EnumDemographyEducationalAttainment?
  employmentStatus      EnumDemographyEmploymentStatus?
  ethnicity             String?
  gender                EnumDemographyGender?
  id                    String                               @id @default(cuid())
  maritalStatus         EnumDemographyMaritalStatus?
  updatedAt             DateTime                             @updatedAt
}

model Identification {
  address       String?
  contactNumber String?
  createdAt     DateTime                         @default(now())
  dateOfBirth   DateTime?
  email         String?
  fullName      String?
  gender        EnumIdentificationGender?
  id            String                           @id @default(cuid())
  maritalStatus EnumIdentificationMaritalStatus?
  nationalId    String?
  updatedAt     DateTime                         @updatedAt
}

model SourceOfIncome {
  amountFromPrimarySource   Float?
  amountFromSecondarySource Float?
  createdAt                 DateTime                                   @default(now())
  frequencyOfIncome         EnumSourceOfIncomeFrequencyOfIncome?
  id                        String                                     @id @default(cuid())
  otherSourcesOfIncome      String?
  primarySourceOfIncome     EnumSourceOfIncomePrimarySourceOfIncome?
  secondarySourceOfIncome   EnumSourceOfIncomeSecondarySourceOfIncome?
  updatedAt                 DateTime                                   @updatedAt
}

model Crime {
  createdAt             DateTime                @default(now())
  dateOfIncident        DateTime?
  id                    String                  @id @default(cuid())
  incidentsOfCrime      String?
  locationOfIncident    String?
  natureOfCrime         EnumCrimeNatureOfCrime?
  outcome               String?
  reportedToAuthorities Boolean?
  updatedAt             DateTime                @updatedAt
}

model PoliticalParticipation {
  activeInCommunityOrganizations  Boolean?
  createdAt                       DateTime @default(now())
  hasVotedInLastElection          Boolean?
  id                              String   @id @default(cuid())
  interestInPublicOffice          Boolean?
  participationInPoliticalRallies Boolean?
  politicalPartyAffiliation       String?
  updatedAt                       DateTime @updatedAt
  voterRegistrationStatus         Boolean?
}

model Housing {
  constructionMaterial         EnumHousingConstructionMaterial?
  createdAt                    DateTime                         @default(now())
  electricityAccess            Boolean?
  houseType                    EnumHousingHouseType?
  id                           String                           @id @default(cuid())
  neighborhoodType             EnumHousingNeighborhoodType?
  numberOfRooms                Int?
  ownershipStatus              EnumHousingOwnershipStatus?
  proximityToEssentialServices String?
  updatedAt                    DateTime                         @updatedAt
  yearBuilt                    Int?
}

model HealthAndOtherCharacteristics {
  accessToHealthcare    EnumHealthAndOtherCharacteristicsAccessToHealthcare?
  chronicDiseases       String?
  createdAt             DateTime                                             @default(now())
  disabilityStatus      Boolean?
  healthInsuranceStatus Boolean?
  id                    String                                               @id @default(cuid())
  primaryHealthProvider String?
  updatedAt             DateTime                                             @updatedAt
  vaccinationStatus     Boolean?
}

model EducationAndLiteracy {
  createdAt                    DateTime                                              @default(now())
  currentlyEnrolled            Boolean?
  fieldOfStudy                 String?
  highestEducationalAttainment EnumEducationAndLiteracyHighestEducationalAttainment?
  id                           String                                                @id @default(cuid())
  literacyLevel                EnumEducationAndLiteracyLiteracyLevel?
  schoolAddress                String?
  schoolName                   String?
  updatedAt                    DateTime                                              @updatedAt
  vocationalTraining           Boolean?
}

model CropFarming {
  accessToAgriculturalSupport Boolean?
  annualYield                 Float?
  areaCultivated              Float?
  commonPestsAndDiseases      String?
  createdAt                   DateTime                         @default(now())
  farmingEquipmentUsed        String?
  id                          String                           @id @default(cuid())
  irrigationMethod            EnumCropFarmingIrrigationMethod?
  typeOfCropsGrown            String?
  updatedAt                   DateTime                         @updatedAt
}

model Climate {
  accessToClimateInformation Boolean?
  averageAnnualRainfall      Float?
  averageTemperature         Float?
  climateType                EnumClimateClimateType?
  commonWeatherEvents        String?
  createdAt                  DateTime                @default(now())
  id                         String                  @id @default(cuid())
  impactOnLivelihood         String?
  seasonalChanges            String?
  updatedAt                  DateTime                @updatedAt
}

model EconomicActivity {
  createdAt          DateTime                              @default(now())
  employmentStatus   EnumEconomicActivityEmploymentStatus?
  hoursWorkedPerWeek Int?
  id                 String                                @id @default(cuid())
  industry           String?
  monthlyIncome      Float?
  occupation         String?
  typeOfEmployment   EnumEconomicActivityTypeOfEmployment?
  updatedAt          DateTime                              @updatedAt
  workplaceAddress   String?
}

model WaterAndSanitation {
  accessToSanitationFacility Boolean?
  createdAt                  DateTime                                          @default(now())
  frequencyOfWasteCollection EnumWaterAndSanitationFrequencyOfWasteCollection?
  id                         String                                            @id @default(cuid())
  sanitationFacilityType     EnumWaterAndSanitationSanitationFacilityType?
  updatedAt                  DateTime                                          @updatedAt
  wastewaterDisposalMethod   EnumWaterAndSanitationWastewaterDisposalMethod?
  waterAvailability          EnumWaterAndSanitationWaterAvailability?
  waterQuality               EnumWaterAndSanitationWaterQuality?
  waterSource                EnumWaterAndSanitationWaterSource?
}

model Nutrition {
  accessToNutritiousFood Boolean?
  createdAt              DateTime                    @default(now())
  dietaryIntake          String?
  height                 Float?
  id                     String                      @id @default(cuid())
  mealFrequency          EnumNutritionMealFrequency?
  nutritionalSupplements String?
  specialDietaryNeeds    String?
  updatedAt              DateTime                    @updatedAt
  weight                 Float?
}

model AgricultureFarming {
  accessToAgriculturalSupport Boolean?
  annualYield                 Float?
  areaCultivated              Float?
  commonPestsAndDiseases      String?
  createdAt                   DateTime                                @default(now())
  farmingEquipmentUsed        String?
  id                          String                                  @id @default(cuid())
  irrigationMethod            EnumAgricultureFarmingIrrigationMethod?
  typeOfCropsGrown            String?
  updatedAt                   DateTime                                @updatedAt
}

model LivestockRaising {
  accessToVeterinaryServices Boolean?
  annualProduction           Float?
  commonDiseases             String?
  createdAt                  DateTime                              @default(now())
  feedingPractices           String?
  id                         String                                @id @default(cuid())
  numberOfAnimals            Int?
  purposeOfRaising           EnumLivestockRaisingPurposeOfRaising?
  typeOfLivestockRaised      String?
  updatedAt                  DateTime                              @updatedAt
}

model Fishing {
  accessToFishingSupport Boolean?
  annualCatch            Float?
  commonIssuesFaced      String?
  commonSpeciesCaught    String?
  createdAt              DateTime                  @default(now())
  fishingGearUsed        String?
  fishingGrounds         String?
  id                     String                    @id @default(cuid())
  typeOfFishing          EnumFishingTypeOfFishing?
  updatedAt              DateTime                  @updatedAt
}

model OtherSourceOfIncome {
  amount            Float?
  createdAt         DateTime                          @default(now())
  frequency         EnumOtherSourceOfIncomeFrequency?
  id                String                            @id @default(cuid())
  notes             String?
  sourceDescription String?
  updatedAt         DateTime                          @updatedAt
}

model User {
  createdAt DateTime @default(now())
  email     String?  @unique
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}

